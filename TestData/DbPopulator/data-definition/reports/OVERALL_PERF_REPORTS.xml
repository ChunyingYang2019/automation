
<Reports>
  <Report id="PH_Report_OVERALL_PERF_2" group="PH_SYS_REPORT_SysPerf,PH_SYS_REPORT_Freq">
     <Name>Top Devices By CPU Util </Name> 
     <CustomerScope groupByEachCustomer="true">
        <Include all="true" /> 
        <Exclude /> 
     </CustomerScope>
     <Description> Ranks the devices by average cpu utilization over a window </Description> 
     <SelectClause numEntries="All">
         <AttrList> hostName,cpuName, AVG(cpuUtil) </AttrList> 
     </SelectClause>
     <OrderByClause><AttrList> AVG(cpuUtil) DESC</AttrList></OrderByClause>
     <ReportInterval><Window unit="Hourly" val="1"/></ReportInterval>
     <PatternClause window="3600" >
         <SubPattern displayName="Filter_PERF_MON_2" name="Filter_PERF_MON_2">
           <SingleEvtConstr> eventType = "PH_DEV_MON_SYS_CPU_UTIL" </SingleEvtConstr>
           <GroupByAttr> hostName,cpuName </GroupByAttr> 
         </SubPattern>
     </PatternClause>
      <RelevantFilterAttr>hostIpAddr</RelevantFilterAttr>
  </Report>

