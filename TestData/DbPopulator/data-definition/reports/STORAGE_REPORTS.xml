<Reports>

  <Report id="PH_Report_Storage_1" group="PH_SYS_REPORT_Storage">
    <Name>Top Reporting Storage Devices</Name>
    <CustomerScope groupByEachCustomer="true"> <Include all="true"/> <Exclude/> </CustomerScope>
    <Description>Ranks reporting storage systems by event count</Description>
    <SelectClause numEntries="All">
      <AttrList>hostIpAddr,COUNT(*)</AttrList>
    </SelectClause>
    <OrderByClause><AttrList>COUNT(*) DESC</AttrList></OrderByClause>
    <ReportInterval><Window unit="Hourly" val="1"/></ReportInterval>
    <PatternClause window="3600">
      <SubPattern displayName="Filter" name="Filter_Storage">
        <SingleEvtConstr>hostIpAddr IN (Group@PH_SYS_DEVICE_Storage)</SingleEvtConstr>
        <GroupByAttr>hostIpAddr</GroupByAttr>
      </SubPattern>
    </PatternClause>
    <RelevantFilterAttr>hostIpAddr</RelevantFilterAttr>
  </Report>

  <Report id="PH_Report_Storage_2" group="PH_SYS_REPORT_Storage">
    <Name>Top Reporting Storage Device Event Types By Count</Name>
    <CustomerScope groupByEachCustomer="true"> <Include all="true"/> <Exclude/> </CustomerScope>
    <Description>Ranks reporting storage systems event types by count</Description>
    <SelectClause numEntries="All">
      <AttrList>hostName,hostIpAddr,eventType, COUNT(*)</AttrList>
    </SelectClause>
    <OrderByClause><AttrList>COUNT(*) DESC</AttrList></OrderByClause>
    <ReportInterval><Window unit="Hourly" val="1"/></ReportInterval>
    <PatternClause window="3600">
      <SubPattern displayName="Filter" name="Filter_Storage">
        <SingleEvtConstr>hostIpAddr IN (Group@PH_SYS_DEVICE_Storage)</SingleEvtConstr>
        <GroupByAttr>hostName,hostIpAddr,eventType</GroupByAttr>
      </SubPattern>
    </PatternClause>
    <RelevantFilterAttr>hostIpAddr</RelevantFilterAttr>
  </Report>

  <Report id="PH_Report_Storage_3" group="PH_SYS_REPORT_Storage,PH_SYS_REPORT_Freq">
    <Name>Top Storage Devices By CPU Util </Name> 
    <CustomerScope groupByEachCustomer="true">
      <Include all="true" /> 
        <Exclude /> 
     </CustomerScope>
     <Description> Ranks the storage devices by average cpu utilization over a window </Description> 
     <SelectClause numEntries="All">
         <AttrList>hostName,hostIpAddr,AVG(cpuUtil),MAX(cpuUtil),MIN(cpuUtil)</AttrList> 
     </SelectClause>
     <OrderByClause><AttrList> AVG(cpuUtil) DESC</AttrList></OrderByClause>
     <ReportInterval><Window unit="Hourly" val="1"/></ReportInterval>
     <PatternClause window="3600" >
         <SubPattern displayName="Filter_PERF_MON_2" name="Filter_PERF_MON_2">
           <SingleEvtConstr> eventType = "PH_DEV_MON_SYS_CPU_UTIL" AND hostIpAddr IN (Group@PH_SYS_DEVICE_Storage)</SingleEvtConstr>
           <GroupByAttr>hostName,hostIpAddr</GroupByAttr> 
         </SubPattern>
     </PatternClause>
      <RelevantFilterAttr>hostIpAddr</RelevantFilterAttr>
  </Report>

  <Report id="PH_Report_Storage_4" group="PH_SYS_REPORT_Storage">
    <Name>Top NetApp storage devices by disk activity </Name> 
    <CustomerScope groupByEachCustomer="true">
      <Include all="true" /> 
      <Exclude /> 
    </CustomerScope>
    <Description> Ranks the storage devices by disk activity and presents other metrics </Description> 
    <SelectClause numEntries="All">
      <AttrList>hostName,hostIpAddr,MIN(cacheAgeMin),AVG(cifsOpsPerSec),AVG(nfsOpsPerSec),AVG(diskReadKBytesPerSec),AVG(diskWriteKBytesPerSec),AVG(netSentKBytesPerSec),AVG(netRecvdKBytesPerSec) </AttrList> 
    </SelectClause>
    <OrderByClause><AttrList> AVG(diskReadKBytesPerSec) DESC,AVG(diskWriteKBytesPerSec) DESC </AttrList></OrderByClause>
    <ReportInterval><Window unit="Hourly" val="1"/></ReportInterval>
    <PatternClause window="3600" >
      <SubPattern displayName="Filter_PERF_MON_2" name="Filter_PERF_MON_2">
        <SingleEvtConstr> eventType = "PH_DEV_MON_NETAPP_NFS_MET" </SingleEvtConstr>
        <GroupByAttr>hostName,hostIpAddr</GroupByAttr> 
      </SubPattern>
    </PatternClause>
    <RelevantFilterAttr>hostIpAddr</RelevantFilterAttr>
  </Report>
  
